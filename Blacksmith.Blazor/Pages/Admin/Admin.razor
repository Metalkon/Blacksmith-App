@page "/admin"
<!-- attribute [Authorize] -->
@inject HttpClientTokenService client
<PageTitle>Admin</PageTitle>

@if (pageState == "ItemManager")
{
    <Blacksmith.Blazor.Pages.Admin.ItemManager.AdminItemManager />
}
@if (pageState == "UserManager")
{
    <Blacksmith.Blazor.Pages.Admin.ItemManager.AdminUserManager />
}

@code {
    private string pageState;

    protected override async Task OnInitializedAsync()
    {
        pageState = "ItemManager";
    }
}
